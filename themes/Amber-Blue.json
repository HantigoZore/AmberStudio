	"tokenColors": [
    {
        "scope": [
            "comment",
            "punctuation.definition.comment"
        ],
        "settings": {
            "foreground": "#ABB2BF", 
            "fontStyle": "italic"
        }
    },
    {

        "scope": [
            "keyword.control",
            "keyword.operator.new",
            "storage.type.function.arrow",
            "keyword.other.debugger"
        ],
        "settings": {
            "foreground": "#C792EA" 
        }
    },
    {

        "scope": [
            "keyword.operator",
            "keyword.operator.arithmetic",
            "keyword.operator.comparison",
            "keyword.operator.assignment",
            "keyword.operator.bitwise",
            "keyword.operator.logical"
        ],
        "settings": {
            "foreground": "#00bfff" 
        }
    },
    {

        "scope": [
            "entity.name.function",
            "support.function",
            "meta.function-call",
            "entity.name.method"
        ],
        "settings": {
            "foreground": "#00bfff" 
        }
    },
    {

        "scope": [
            "variable.parameter",
            "variable.other.readwrite",
            "variable.other.property",
            "variable.other.object",
            "entity.name.variable",
            "support.variable"
        ],
        "settings": {
            "foreground": "#ffe600" 
        }
    },
    {

        "scope": [
            "constant.numeric",
            "constant.language", 
            "support.constant"
        ],
        "settings": {
            "foreground": "#FF5370" 
        }
    },
    {

        "scope": [
            "string",
            "punctuation.definition.string",
            "markup.inline.raw" 
        ],
        "settings": {
            "foreground": "#98C379" 
        }
    },
    {
 
        "scope": [
            "entity.name.type",
            "entity.name.class",
            "storage.type",
            "support.class",
            "entity.name.module" 
        ],
        "settings": {
            "foreground": "#F69D50" 
        }
    },
    {

        "scope": [
            "entity.name.tag"
        ],
        "settings": {
            "foreground": "#F07178" 
        }
    },
    {

        "scope": [
            "entity.other.attribute-name"
        ],
        "settings": {
            "foreground": "#FFCB6B" 
        }
    },
    {

        "scope": [
            "punctuation.separator",
            "punctuation.accessor",
            "punctuation.terminator",
            "punctuation.section",
            "punctuation.definition.tag"
        ],
        "settings": {
            "foreground": "#e1e4e8" 
        }
    },
    {

        "scope": [
            "variable.language"
        ],
        "settings": {
            "foreground": "#FF5370" 
        }
    }
],
